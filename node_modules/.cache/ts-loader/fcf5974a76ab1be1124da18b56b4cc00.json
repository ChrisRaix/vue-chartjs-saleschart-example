yBAAyB;oBACzB,IAAM,KAAK,GAAC,IAAI,CAAC,QAAQ,CAAC,WAAW,CAAC,YAAY,CAAC,CAAC,KAAe,CAAC;oBAEpE,2BAA2B;oBAC3B,IAAM,KAAK,GAAC,OAAO,CAAC,WAAW,CAAC,MAAM,CAAC,CAAC,MAAM,CAAC,KAAK,CAAC,GAAC,GAAU,CAAC;oBAEjE,OAAU,KAAK,UAAK,KAAO,CAAC;gBAC7B,CAAC;aACD;SACD;KACD;CACD,CAAC,CAAC","sourcesContent":["import Vue from 'vue'\nimport App from './App.vue'\nimport Chart from 'chart.js'\nimport numeral from 'numeral';\n//import Chroma from 'chroma-js'\n\nVue.config.productionTip=false\n\nnew Vue({\n\trender: h => h(App)\n}).$mount('#app')\n\nconst canvas=document.getElementById(\"mychart\") as HTMLCanvasElement;\nconst ctx=canvas.getContext('2d') as any;\n//const borderColor1=Chroma('rgba(50, 115, 220, 0.5)').alpha(0.6) as any;\n\n\nconst mychart=new Chart(ctx,{\n\ttype: 'line',\n\tdata: {\n\t\tdatasets: [\n\t\t\t{\n\t\t\t\tlabel: '2018 Sales',\n\t\t\t\tborderColor: 'rgba(50, 115, 220, 0.5)',\n\t\t\t\tbackgroundColor: 'rgba(50, 115, 220, 0.1)',\n\t\t\t\tdata: [300,700,450,750,450]\n\t\t\t},\n\t\t\t{\n\t\t\t\tlabel: '2017 Sales',\n\t\t\t\tborderColor: 'rgba(255, 56, 96, 0.5)',\n\t\t\t\tbackgroundColor: 'rgba(255, 56, 96, 0.1)',\n\t\t\t\tdata: [600,550,750,250,700]\n\t\t\t}\n\t\t]\n\t},\n\toptions: {\n\t\tscales: {\n\t\t\txAxes: [{\n\t\t\t\ttype: 'category',\n\t\t\t\tlabels: ['Jan','Feb','Mar','Apr','May']\n\n\t\t\t}],\n\t\t\tyAxes: [{\n\t\t\t\tticks: {\n\t\t\t\t\tbeginAtZero: true,\n\t\t\t\t\tcallback: value => numeral(value).format('0,0')+'€'\n\t\t\t\t}\n\t\t\t}]\n\t\t},\n\t\ttooltips: {\n\t\t\tmode: 'index',\n\t\t\tcallbacks: {\n\t\t\t\tlabel(tooltipItem,data) {\n\t\t\t\t\t// Get the dataset label.\n\t\t\t\t\tconst label=data.datasets[tooltipItem.datasetIndex].label as string;\n\n\t\t\t\t\t// Format the y-axis value.\n\t\t\t\t\tconst value=numeral(tooltipItem.yLabel).format('0,0')+'€' as any;\n\n\t\t\t\t\treturn `${label}: ${value}`;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n});\n\n\n\n\n\n"]}]}